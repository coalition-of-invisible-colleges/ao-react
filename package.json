    "ipfs-vue": "^0.35.0-rc.6",
    "kefir": "^3.8.2",
    "lightning-client": "^0.5.2-beta.8",
    "linkifyjs": "^2.1.8",
    "lodash": "^4.17.5",
    "moment": "^2.22.2",
    "nan": "^2.13.2",
    "opc": "^1.1.3",
    "path": "^0.12.7",
    "pug-plain-loader": "^1.0.0",
    "qrcode-generator": "^1.3.1",
    "rethinkdb": "^2.3.3",
    "socket.io": "^2.0.4",
