{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES5",
    "sourceMap": true,
    "jsx": "react",
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "outDir": "./dist",
    "lib": ["ES6", "ES5", "DOM"]
  }
}
